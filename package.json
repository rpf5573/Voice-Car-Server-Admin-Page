    "webpack-cli": "^3.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^7.0.0",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/morgan": "^1.7.35",
